[user]
	name = mutilx9
	email = mutilx9@gmail.com

[alias]
	go = checkout
	ci = commit
	st = status -s
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	type = cat-file -t
	dump = cat-file -p
	tags = tag -l
	branches = branch -a
	remotes = remote -v

[color]
	ui = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = yellow
	untracked = cyan

[merge]
	ff = false
	log = true
	tool = vimdiff

[pull]
	ff = only

[push]
	default = current

[log]
	date = relative
